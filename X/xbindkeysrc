"amixer set Master 1%-"
    XF86AudioLowerVolume

"amixer set Master 1%+"
    XF86AudioRaiseVolume

"amixer set Master toggle; amixer get Master | grep off > /dev/null && xset led 3 || xset -led 3"
    XF86AudioMute

"cantera-term"
    Control + Alt + Return

"xgamma -gamma 1"
    Control + Alt + backslash

"xgamma -gamma $(echo "$(xgamma 2>&1 | awk '{print $3}' | cut -d, -f 1) * 1.1" | bc)"
    Control + Alt + bracketright

"xgamma -gamma $(echo "$(xgamma 2>&1 | awk '{print $3}' | cut -d, -f 1) * 0.9" | bc)"
    Control + Alt + bracketleft

"exec dmenu_run"
    Control + Alt + d

"exec emacs"
    Control + Alt + e

"exec xcalib -i -a"
    Control + Alt + i

"urxvt -e ~/bin/calc"
    Control + Alt + k

"exec cantera-lock"
    Control + Alt + l

"exec chromium-browser `xclip -o`"
    Control + Alt + v

"xmag -source 32x32"
    Control + Alt + z

"xmag -source 64x64"
    Control + Alt + x

"xmag -source 128x128"
    Control + Alt + c

"exec zathura"
    Control + Alt + p

"exec setxkbmap us"
  Control + Alt + u

"exec setxkbmap no"
  Control + Alt + n

"exec amixer set Master 10%-"
  Control + Down

"exec amixer set Master 10%+"
  Control + Up

"exec ~/src/cc-y-download/cc-y-download.sh"
  Control + Alt + h
